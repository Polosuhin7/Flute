{
  "name": "mobile",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "./",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:ios": "expo start:web --ios",
    "build:android": "expo build:android -t apk",
    "build:ios": "expo build:ios -t archive",
    "build:web": "expo build:web && yarn after:build:web",
    "after:build:web": "cd .. && cp -r ./mobile/web-build ./backend/public/ && mv ./backend/public/web-build ./backend/public/web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^3",
    "@react-native-async-storage/async-storage": "^1.13.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.9.4",
    "axios": "^0.21.1",
    "expo": "~41.0.1",
    "expo-cli": "4.7.3",
    "expo-app-loading": "^1.0.3",
    "expo-font": "~9.1.0",
    "expo-linking": "~2.2.3",
    "expo-localization": "~10.1.0",
    "expo-location": "~12.0.4",
    "expo-permissions": "~12.0.1",
    "expo-status-bar": "~1.0.4",
    "geolib": "^3.3.1",
    "i18n-js": "^3.8.0",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-41.0.0.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-maps": "0.27.1",
    "react-native-maps-directions": "^1.8.0",
    "react-native-reanimated": "~2.1.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.0.0",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-web": "~0.13.12",
    "reanimated-bottom-sheet": "^1.0.0-alpha.22"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/webpack-config": "^0.12.82",
